#include<iostream>

using namespace std;

int main()
{
    int n;
    int x;
    int y;
    int sum1=0,sum2=0;

    cin>>n;

    for (int i = 0; i < n; i++)
    {
        cin>>x>>y;
        sum1+=x;
        sum2+=y;
    }

    if(sum1 > sum2)
    {
        cout << "Takahashi" <<endl;
    }

    else if(sum2 > sum1)
    {
         cout << "Aoki" << endl;
    }

    else
    {
        cout << "Draw" << endl;
    }
    
    return 0;
}
